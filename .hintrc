{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off",
        "link-name": "off"
      }
    ],
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "iframe[loading]",
          "meta[name=theme-color]"
        ]
      }
    ],
    "axe/structure": [
      "default",
      {
        "list": "off"
      }
    ],
    "axe/forms": [
      "default",
      {
        "label": "off"
      }
    ],
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-text-size-adjust",
          "-ms-flex",
          "-ms-touch-action",
          "-webkit-user-modify",
          "overscroll-behavior",
          "scrollbar-width",
          "scrollbar-color",
          "-webkit-overflow-scrolling",
          "-webkit-tap-highlight-color",
          "-webkit-mask-box-image",
          "overflow: clip",
          "backdrop-filter"
        ]
      }
    ],
    "button-type": "off",
    "axe/keyboard": [
      "default",
      {
        "nested-interactive": "off"
      }
    ],
    "meta-viewport": "off"
  }
}